ul.sidebar-links li .sidebar-sub-headers{
    position: fixed;
    padding-left: 50px;
    top: 100px;
    right: -34px;
    width: 150px;
    font-size: 14px;
    padding-right: 30px;
    li{
            height: 30px;
            line-height: 30px;
            list-style: none;
            display: flex;
            position: relative;
        a{
                text-decoration: none!important;
                width: 160px;
                height: 30px;
                line-height: 30px;
                margin-bottom: 10px;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                padding: 0;
        }
    }
}
ul.sidebar-links li ul.sidebar-sub-headers li{
    position:relative;
}
ul.sidebar-links li ul.sidebar-sub-headers li a:after{
    content: "";
    position: absolute;
    width: 5px;
    height: 5px;
    background: #c4c9d2;
    border-radius: 50%;
    top: 50%;
    transform: translateY(-50%);
    left: -21px;
}
ul.sidebar-links li ul.sidebar-sub-headers li a.active:after{

    width: 20px;
    height: 20px;
    background-repeat: no-repeat;
    background-size: contain;
    background-color: #fff;
    transform: translateY(-50%);
    left: -27px;
    background-image :url('data:image/png;base64,')
}
.sidebar {
    font-size: 15px;
    background-color: #fff;
    width: 250px;
    position: fixed;
    z-index: 10;
    margin: 0;
    top: 4.5rem;
    left: 0;
    bottom: 0;
    box-sizing: border-box;
    border-right: 1px solid #eaecef;
    overflow-y: auto;
}
.page {
    padding-left: 250px;
    margin-right: 150px;
}
.theme-default-content:not(.custom){
    max-width: 900px;
}